  "info": {
    "title": "Service API",
    "description": "Service API",
    "version": "1.0.0",
    "contact": {}
  },
  "tags": [],
  "servers": [
    {
      "url": "http://localhost:3002/api"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "description": "JWT token for authenticated endpoints"
      },
      "x-api-key": {
        "type": "apiKey",
        "in": "header",
        "name": "x-api-key",
        "description": "API Key required by API Gateway for all requests (validated at gateway level)"
      }
    },
    "schemas": {
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "User email address",
            "example": "user@example.com",
            "minLength": 5,
            "maxLength": 50
          },
          "password": {
            "type": "string",
            "description": "User password",
            "example": "SecurePassword123!",
            "minLength": 1
          },
          "deviceId": {
            "type": "string",
            "description": "Unique device identifier",
            "example": "device_12345"
          },
          "userAgent": {
            "type": "string",
            "description": "User agent string from the client",
            "example": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "AuthTokensDto": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string",
            "description": "JWT access token",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
          },
          "refreshToken": {
            "type": "string",
            "description": "JWT refresh token",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
          },
          "expiresIn": {
            "type": "number",
            "description": "Token expiration time in seconds",
            "example": 900
          },
          "tokenType": {
            "type": "string",
            "description": "Token type",
            "example": "Bearer"
          }
        },
        "required": [
          "accessToken",
          "refreshToken",
          "expiresIn",
          "tokenType"
        ]
      },
      "LoginResponseDto": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "description": "Success status",
            "example": true
          },
          "message": {
            "type": "string",
            "description": "Response message",
            "example": "Login successful"
          },
          "data": {
            "description": "Authentication tokens",
            "allOf": [
              {
                "$ref": "#/components/schemas/AuthTokensDto"
              }
            ]
          }
        },
        "required": [
          "success",
          "message",
          "data"
        ]
      },
      "LogoutResponseDto": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "description": "Success status",
            "example": true
          },
          "message": {
            "type": "string",
            "description": "Response message",
            "example": "Successfully logged out"
          }
        },
        "required": [
          "success",
          "message"
        ]
      },
      "RefreshTokenDto": {
        "type": "object",
        "properties": {
          "refreshToken": {
            "type": "string",
            "description": "JWT refresh token",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
          },
          "deviceId": {
            "type": "string",
            "description": "Unique device identifier",
            "example": "device_12345"
          },
          "userAgent": {
            "type": "string",
            "description": "User agent string from the client",
            "example": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
          }
        },
        "required": [
          "refreshToken"
        ]
      },
      "RefreshTokenResponseDto": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "description": "Success status",
            "example": true
          },
          "message": {
            "type": "string",
            "description": "Response message",
            "example": "Tokens refreshed successfully"
          },
          "data": {
            "description": "New authentication tokens",
            "allOf": [
              {
                "$ref": "#/components/schemas/AuthTokensDto"
              }
            ]
          }
        },
        "required": [
          "success",
          "message",
          "data"
        ]
      },
      "UserProfileDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "User ID"
          },
          "firstName": {
            "type": "string",
            "description": "User name"
          },
          "lastName": {
            "type": "string",
            "description": "User last name"
          },
          "email": {
            "type": "string",
            "description": "User email"
          },
          "phone": {
            "type": "string",
            "description": "User phone number"
          },
          "isActive": {
            "type": "number",
            "description": "User status",
            "enum": [
              1,
              0
            ]
          },
          "createdAt": {
            "type": "string",
            "description": "User creation timestamp"
          },
          "updatedAt": {
            "type": "string",
            "description": "User last update timestamp"
          }
        },
        "required": [
          "id",
          "firstName",
          "lastName",
          "email",
          "isActive",
          "createdAt",
          "updatedAt"
        ]
      },
      "GoogleRedirectUrlDto": {
        "type": "object",
        "properties": {
          "deviceId": {
            "type": "string",
            "description": "Unique device identifier",
            "example": "device_12345"
          },
          "userAgent": {
