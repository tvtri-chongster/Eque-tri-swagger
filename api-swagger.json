{
  "openapi": "3.0.0",
  "paths": {
    "/auth/login": {
      "post": {
        "description": "Authenticates user with email and password",
        "operationId": "login",
        "parameters": [
          {
            "name": "x-request-id",
            "in": "header",
            "description": "Request ID for tracking",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "x-correlation-id",
            "in": "header",
            "description": "Correlation ID for tracing",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponseDto"
                }
              }
            }
          },
          "401": {
            "description": "No account found with this email",
            "content": {
              "application/json": {
                "schema": {
                  "example": {
                    "message": "No account found with this email. Please check again or sign up.",
                    "error": "Unauthorized",
                    "statusCode": 401
                  }
                }
              }
            }
          }
        },
        "summary": "User login",
        "tags": [
          "Authentication"
        ]
      }
    },
    "/auth/logout": {
      "post": {
        "description": "Logs out user and invalidates session tokens",
        "operationId": "logout",
        "parameters": [
          {
            "name": "x-request-id",
            "in": "header",
            "description": "Request ID for tracking",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "x-correlation-id",
            "in": "header",
            "description": "Correlation ID for tracing",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Logout successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LogoutResponseDto"
                }
              }
            }
          },
          "400": {
            "description": "Invalid logout data",
            "content": {
              "application/json": {
                "schema": {
                  "example": {
                    "message": "Unauthorised",
                    "error": "Unauthorised",
                    "statusCode": 401
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "User logout",
        "tags": [
          "Authentication"
        ]
      }
    },
    "/auth/refresh-token": {
      "post": {
        "description": "Generates new access token using refresh token from request body",
        "operationId": "refreshToken",
        "parameters": [
          {
            "name": "x-request-id",
            "in": "header",
            "description": "Request ID for tracking",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "x-correlation-id",
            "in": "header",
            "description": "Correlation ID for tracing",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RefreshTokenDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Token refresh successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RefreshTokenResponseDto"
                }
              }
            }
          },
          "401": {
            "description": "Invalid refresh token",
            "content": {
              "application/json": {
                "schema": {
                  "example": {
                    "success": false,
                    "message": "Invalid refresh token",
                    "requestId": "12345-67890"
                  }
                }
              }
            }
          }
        },
        "summary": "Refresh access token",
        "tags": [
          "Authentication"
        ]
      }
    },
    "/auth/health": {
      "get": {
        "description": "Returns the health status of the authentication service",
        "operationId": "getHealth",
        "parameters": [
          {
            "name": "x-request-id",
            "in": "header",
            "description": "Request ID for tracking",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "x-correlation-id",
            "in": "header",
            "description": "Correlation ID for tracing",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Service is healthy",
            "content": {
              "application/json": {
                "schema": {
                  "example": {
                    "status": "ok",
                    "service": "lambda4-auth",
                    "timestamp": "2025-07-29T12:00:00.000Z",
                    "requestId": "12345-67890"
                  }
                }
              }
            }
          }
        },
        "summary": "Health check",
        "tags": [
          "Authentication"
        ]
      }
    },
    "/auth/me": {
      "get": {
        "description": "Retrieves the authenticated user's profile information including roles and permissions",
        "operationId": "me",
        "parameters": [],
        "responses": {
          "200": {
            "description": "User profile retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserProfileDto"
                }
              }
            }
          },
          "400": {
            "description": "Failed to retrieve user profile",
            "content": {
              "application/json": {
                "schema": {
                  "example": {
                    "success": false,
                    "message": "Failed to get user profile",
                    "error": "SERVICE_UNAVAILABLE",
                    "statusCode": 503
                  }
                }
              }
            }
          },
          "401": {
            "description": "User not authenticated or token invalid",
            "content": {
              "application/json": {
                "schema": {
                  "example": {
                    "success": false,
                    "message": "User not authenticated or token invalid",
                    "error": "UNAUTHORIZED",
                    "statusCode": 401
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "Get current user profile",
        "tags": [
          "Authentication"
        ]
      }
    },
    "/auth/google/redirect-url": {
      "post": {
        "description": "Generates a Google OAuth2 redirect URL for SSO authentication. The frontend should redirect users to this URL to initiate the Google SSO flow.",
        "operationId": "generateGoogleRedirectUrl",
        "parameters": [],
        "requestBody": {
          "required": true,
          "description": "Device and client information for the SSO request",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GoogleRedirectUrlDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Google SSO redirect URL generated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GoogleRedirectUrlResponseDto"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request data or Google OAuth configuration missing"
          },
          "401": {
            "description": "Google OAuth configuration is not available"
          }
        },
        "summary": "Generate Google SSO redirect URL",
        "tags": [
          "Authentication"
        ]
      }
    },
    "/auth/apple/redirect-url": {
